{
  "name": "signup-flow",
  "private": true,
  "workspaces": [
    "packages/signup-flow",
    "packages/workers/*"
  ],
  "scripts": {
    "fmt": "prettier -w .",
    "build:signup-flow": "BUILD_TIME=$(node -e \"process.stdout.write(new Date().toISOString())\") esbuild packages/signup-flow/src/index.js --bundle --format=iife --global-name=AnvisningerSignupFlow --outfile=dist/signup-flow.js --define:__BUILD_TIME__=\\\"$BUILD_TIME\\\""
  },
  "devDependencies": {
    "esbuild": "^0.27.3",
    "prettier": "^3.0.0"
  }
}
